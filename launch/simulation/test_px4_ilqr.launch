<launch>

	<include file="$(find gazebo_ros)/launch/empty_world.launch">
		<arg name="world_name" value="$(find aerial_hitter)/worlds/empty.world" />
		<arg name="paused" value="false"/>
	</include>


	<rosparam param="wp_x">0</rosparam>
	<rosparam param="wp_y">1.0</rosparam>
	<rosparam param="wp_z">3.0</rosparam>
	<include file="$(find aerial_hitter)/launch/simulation/spawn_mav.launch">
		<arg name="model" value="$(find aerial_hitter)/description/hummingbird_arm.urdf" />
		<arg name="enable_logging" value="true" />
		<arg name="enable_ground_truth" value="true" />
		<arg name="log_file" value="hb1"/>
		<arg name="x" value="0.0"/>
		<arg name="y" value="0.0"/>
		<arg name="z" value="0.40"/>
	</include>
			

